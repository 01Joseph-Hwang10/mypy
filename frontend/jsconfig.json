{
    "compilerOptions": {
      "module": "commonjs",
      "declaration": true,
      "removeComments": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "allowSyntheticDefaultImports": true,
      "target": "es2017",
      "sourceMap": true,
      "baseUrl": "./",
      "paths": {
          "@components/*":["components/*"],
          "@pages/*":["pages/*"],
          "@styles/*":["styles/*"],
          "@redux/*":["redux/*"],
          "@slices/*":["redux/slices/*"],
          "@form/*":["redux/form/*"],
          "@src/*":["src/*"],
          "@hooks/*":["hooks/*"],
          "@functions/*":["functions/*"],
          "@layout/*":["layout/*"],
      }
    }
  }